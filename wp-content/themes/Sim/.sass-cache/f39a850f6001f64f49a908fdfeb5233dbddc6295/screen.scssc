/***************************************************************************************
* BODY ELEMENTS
***************************************************************************************/
*{
	outline-color: $vibrant-color;
}

html, body{
	height: 100%;
}

body {
	font: 12px/16px Arial, sans-serif;
	color: $font-color;
	background: $body-color;
	/*@include grid-background();*/
	@media screen and (max-width: 639px){
		padding: 0 5px 10px;
		line-height: 18px;
	}

	&.page-template-contact-php{
		background: darken($body-color, 10%);
		color: darken($font-color, 10%);

		section.right{
			border-top-color: $font-color;
		}
	}
}
div {}
hr {}
img {
	max-width: 100%;
	margin-bottom: $base-line-height*1.5;
	margin-top: $base-line-height*1.5;

	&.aligncenter{
		display: block;
		margin-left: auto;
		margin-right: auto;
	}

	@media screen and (min-width: 981px){
      max-width: 85%;
  }
}
a {
	color: $link-color;
	@include hover-link;
}

::selection {
	background: lighten($font-color, 25%);
	color: darken($vibrant-color, 8%);
}
::-moz-selection {
	background: lighten($font-color, 25%);
	color: darken($vibrant-color, 8%);
}

/***************************************************************************************
* HEADINGS
***************************************************************************************/

/* All Headings */
h1, h2, h3 {
	font-family: "Arial Black", Arial, sans-serif;
	font-weight: bold;
	text-transform: uppercase;
	color: lighten($font-color, 10%);
}

/* Individual Headings */
h1 {
	font-size: 48px;
	line-height: 48px;
	letter-spacing: -2px;
}
h2 {
	font-size: 36px;
	line-height: 40px;
	letter-spacing: -1px;
}
h3 {
	font-size: 24px;
	line-height: 32px;
	letter-spacing: -1px;
}
h4 {
	color: darken($menu-color, 2%);
	text-transform: lowercase;
	padding-top: $base-line-height;
	margin-bottom: $base-line-height;
}

/***************************************************************************************
* PARAGRAPHS
***************************************************************************************/

/* First Paragraph */
p {
	margin-bottom: 16px;
	@include clearfix;
}

/* Seccond Paragraph */
abbr {}
b {}

/* Blockquote */
blockquote {
	@include put-quotes(24px);
	font-size: 90%;
	line-height: 140%;

	strong{
		font-weight: normal;
		font-size: 130%;
	}
}
cite {}

/* Third Paragraph */
em {
	font-size: 90%;
	font-style: italic;

	a{
		text-decoration: underline;
	}
}
strong {
	font-size: 120%;
	font-weight: bold;
}
small {}

.error404 header h2{
	display: block;
	background: url(../images/red_arrow.png) no-repeat 440px 0;
}

/***************************************************************************************
* LISTS
***************************************************************************************/

/* All Lists */
dl, ol, ul {}

/* Definition Lists */
dl {}
dt {}
dd {}

/* Ordered Lists */
ol {}
ol li {}

/* Unordered Lists */
ul {}
ul li {}

/***************************************************************************************
* FORMS
***************************************************************************************/

form {
	position: relative;
}
fieldset {}
legend {}
label {}

/* Inputs */
input, textarea {
	display: block;
	border: 1px solid darken($body-color, 20%);
	text-transform: lowercase;
	color: $link-color;
	background: lighten($body-color, 20%);
	padding: 2px 5px;
	width: 70%;
	margin: 0 0 $base-font-size/2;
	@include transition(all .2s);

	&.wpcf7-submit{
		background: $link-color;
		color: $body-color;
		width: auto;
		padding: 5px;
	}

	&:focus{
		padding-top: 6px;
		padding-bottom: 6px;
		outline: none;
		border: 1px dotted $vibrant-color;
		font-size: 130%;
	}
}
textarea {}

/* Other */
select {}
optgroup {}

/***************************************************************************************
* TABLES
***************************************************************************************/

table {}
caption {}
tr {}

/* Head */
thead {}
th {}

/* Body */
tbody {}
td {}

/* Foot */
tfoot {}

/***************************************************************************************
* OTHER
***************************************************************************************/

address {}
code {}
pre {}

/***************************************************************************************
* LAYOUT
***************************************************************************************/

#main{
	margin-right: 5%;
	float: right;
	width: 60%;
	@include clearfix;
	clear: both;
  min-height: 90%;
  height: auto !important;
  height: 90%;

	@media screen and (max-width: 980px) and (min-width: 640px){
      width: 600px;
      float: none;
      margin: 0 auto;
  }
  @media screen and (max-width: 639px){
      width: auto;
      float: none;
  }
}

/***********************************************************************
* HEADER
***************/

header.nav{

	text-transform: lowercase;
	font-size: 110%;
	@include pie-clearfix;
	padding-bottom: $base-line-height*6;

	ul{
		@include horizontal-list;
		overflow: visible;
	}

	a{
		display: block;
		margin: 12px 10px 0;
		height: 50px;
		color: $font-color;

		&:hover{
			text-decoration: none;
			color: $link-color;
		}
	}

	li{
		position: relative;
		padding: 16px 5px 3px;
		border-bottom: 2px solid $menu-color;
		@include transition(background .3s);

		&:hover, &.active{
			border-bottom-color: $vibrant-color;
		}

		&:hover{
			background-color: $bg-menu-color;
		}

		&.icon{
			padding: 3px 5px 16px;

			a{
				display: block;
				@include hide-text;
			}
		}

		&.home_item{
			a{
				@include sprites-sprite(home_icon);
				width: 22px;
			}
		}

		&.contact_item{
			a{
				@include sprites-sprite(contact_icon);
				width: 51px;
			}
		}

		ul.dropdown-menu{
			position: absolute;
			z-index: 100;
			background: $dropmenu-color;
			top: 82px;
			left: -999em;
			width: auto;
			padding: 0 8px 5px;

			li{
				float: none;
				display: block;
				font-size: $base-font-size - 2;
				border: none;
				padding: 0;

				span{
					color: $link-color;
					text-transform: uppercase;
					margin-top: 10px;
					display: block;
				}

				a{
					margin: 0;
					height: auto;

					&:hover{
						color: $vibrant-color;
					}
				}
			}
		}

		&:hover ul.dropdown-menu{
			left: 0;
		}
	}
}

/***********************************************************************
* ASIDE CONTENTS
***************/

aside#extra_content{
	position: fixed;
	clear: both;
	bottom: 3%;
	left: 5%;
	width: 260px;

	@media screen and (max-width: 980px){
			@include mobile-version;
      max-width: 400px;
      margin: 20px auto;
      padding-right: 200px;
      overflow: visible !important;
  }

  @media screen and (max-width: 639px){
			max-width: auto;
			width: 70%;
			padding-right: 0;
			margin: 20px 0;
  }
}

nav.arrows{
	@include hide-mobile;

	a{
		position: fixed;
		background: $link-color;
		color: $body-color;
		top: 45%;
		padding: 2px;
		line-height: 10px;
		font-size: 16px;

		&.prev{
			left: 0;
		}

		&.next{
			right: 0;
		}

		&:hover{
			color: $vibrant-color;
		}
	}
}

aside.language{
	position: fixed;
	bottom: 10px;
	right: 10px;
	text-transform: lowercase;

	a{
		color: $special-link-color;
	}

	@media screen and (max-width: 639px){
		@include mobile-version;
	}
}

/***********************************************************************
* MAIN CONTENT
***************/

#content{
	position: relative;
	@include clearfix;

	#intern-footer{
		position: absolute;
		bottom: 0;
		left: 45%;
		width: 400px;

		@media screen and (max-width: 639px){
			@include mobile-version;
			float: left;
			clear: both;
		}

		@include mobile-divider;
	}

	@media screen and (min-width: 640px){
		form{
			top: -$base-line-height*3;
		}
	}
}

article{

	h1{
		@include put-quotes(48px);
	}

	h2{
		@include put-quotes(36px);
	}

	h3{
		@include put-quotes(28px);
	}

	header{
		margin-bottom: $base-line-height*3;

		h2{
			padding-bottom: $base-line-height*2;
		}
	}

	section.full{
		width: 70%;

		@media screen and (max-width: 560px){
			width: auto;
		}
	}

	section.blog{
		float: left;

		p, h1, h2, h3, h4, h5, h6, blockquote{
			width: 50%;
		}

		@media screen and (max-width: 639px){
			@include mobile-version;
			
			p, h1, h2, h3, h4, h5, h6, blockquote{
				width: 70%;
			}
		}
	}

	section.left, section.right{
		width: 35%;
		float: left;
		margin-right: 10%;

		@media screen and (max-width: 639px){
			width: auto;
			margin-right: 10%;
			float: none;
		}
	}

	section.right{
		margin-right: 20%;
		@include mobile-divider;
	}

	section.left{
		@include clearfix;
	}
}

section.clients{
	width: 100%;

	img{
		padding: 0 25px 10px 0;
		opacity: .5;
		@include transition;

		&:hover{
			opacity: 1;
		}
	}
}


a#coordinates{
	height: 34px;
	width: 136px;
	@include sprites-sprite(big_coord);
	position: relative;
	left: -7px;
}

/***********************************************************************
* FOOTER
***************/

footer#footer{
	clear: both;
	float: right;
	margin-top: $base-line-height;
	width: 100%;

	@media screen and (min-width: 640px){
		margin-right: 10px;
	}

	section{
		float: right;
		margin: 10px;

		@media screen and (max-width: 639px){
			@include mobile-version;
			float: left;
			clear: both;
		}
	}

	a.icon{
		display: block;
		@include hide-text;
		float: left;
		margin-right: 20px;
		@include show-desktop;

		&.home_item{
			@include sprites-sprite(home_icon);
			height: 32px;
			width: 22px;
		}
		&.contact_item{
			@include sprites-sprite(contact_icon);
			height: 34px;
			width: 51px;
		}
	}
}

p.telephones{
	font-size: 90%;
}

a.social{
		@include hide-text;
		display: block;
		float: left;
		height: 19px;
		width: 9px;
		margin: 6px 8px 0;
	}

	a.facebook{
		@include sprites-sprite(facebook);
		display: none;
	}

	a.twitter{
		@include sprites-sprite(twitter);
		display: none;
	}

	a.coordinates{
		@include sprites-sprite(coord);
		height: 31px;
		width: 84px;
		margin-top: 0;
	}

	nav.tree_menu{
		float: right;
		display:inline;
		font-size: 90%;
		text-transform: lowercase;
		margin: 10px 20px;

		ul{
			margin-top: $base-line-height/2;
						
			a{
				color: $menu-color;
			}
		}

		@media screen and (max-width: 639px){
			@include mobile-version;
		}
	}:
